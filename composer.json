{
  "name": "hipay/mirakl-silex-integration",
  "description": "Exemple integration of the mirakl-hipay-connector with silex",
  "version": "1.0.3",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "Ivanis KouamÃ©",
      "email": "ivanis.kouame@smile.fr"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/hipay/hipay-wallet-cashout-mirakl"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "doctrine/orm": "^2.4",
    "symfony/console": "^2.6",
    "symfony/yaml": "^2.6",
    "hipay/mirakl-connector" : "1.0.0",
    "incenteev/composer-parameter-handler": "^2.0",
    "gedmo/doctrine-extensions" : "^2.4",
    "swiftmailer/swiftmailer": "^5.0",
    "monolog/monolog": "^1.17",
    "symfony/validator": "^2.6",
    "doctrine/annotations": "^1.2",
    "nesbot/carbon": "^1.21",
    "silex/silex": "~1.3",
    "symfony/doctrine-bridge": "^2.8",
    "mustache/mustache": "^2.9"
